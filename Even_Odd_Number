//Even Number
#include<stdio.h>
main() {
    int a[] = {2,3,4,5,6,7,8,5};

    for(int i=0; i<=a[i]; i++){
        if(a[i]%2==0) {
            printf("Even Number: %d\n",a[i]);
        }
    }
}


//Odd Number
#include<stdio.h>
main() {
    int a[] = {2,3,4,5,6,7,8,5};

    for(int i=0; i<=a[i]; i++){
        if(a[i]%2==1) {
            printf("Odd Number: %d\n",a[i]);
        }
    }
}
